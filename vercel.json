{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist",
                "installCommand": "npm install",
                "buildCommand": "npm run build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/index.html"
        }
    ]
}